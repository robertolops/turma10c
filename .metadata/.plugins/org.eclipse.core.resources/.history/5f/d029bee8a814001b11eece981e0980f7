package br.com.ecommerce.implementacao;

import br.com.ecommerce.beans.Cliente;
import br.com.ecommerce.beans.Endereco;
import br.com.ecommerce.beans.Venda;
import br.com.ecommerce.util.Magica;

public class ImplementaVenda {

	public static void main(String[] args) {
		Venda venda = new Venda( 
				Magica.i("Nota Fiscal"),
				Magica.f("Total"),
				Magica.f("Desconto"),
				Magica.s("Data"),
				new Cliente (
						Magica.i("ID"),
						Magica.s("Nome do Cliente"),
						Magica.s("Telefone"),
						Magica.s(endereco (
								Magica.s("Logradouro"),
								Magica.s("Numero"),
								Magica.s("Complemento"),
								Magica.s("Bairro"),
								Magica.s("cidade"),
								Magica.s("Estado"),
								Magica.s("CEP")
								)
								)
						),
				new Endereco (
						Magica.s("Logradouro"),
						Magica.s("Numero"),
						Magica.s("Complemento"),
						Magica.s("Bairro"),
						Magica.s("cidade"),
						Magica.s("Estado"),
						Magica.s("CEP")
						)
				)
			)
System.out.println(venda.getAll());

	}

}
